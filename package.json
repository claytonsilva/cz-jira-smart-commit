{
  "name": "cz-taiga-smart-commit",
  "version": "2.0.1",
  "description": "A commitizen adapter for Taiga",
  "main": "index.js",
  "scripts": {
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/claytonsilva/cz-taiga-smart-commit.git"
  },
  "keywords": [
    "commitizen",
    "commitizen adapter",
    "taiga",
    "taiga smart commit"
  ],
  "author": "Clayton Silva",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/claytonsilva/cz-taiga-smart-commit.git/issues"
  },
  "homepage": "https://github.com/claytonsilva/cz-taiga-smart-commit.git#readme",
  "devDependencies": {
    "babel": "5.8.21",
    "chai": "3.2.0",
    "mocha": "2.2.5",
    "nodemon": "1.4.0",
    "proxyquire": "^1.7.10",
    "sinon": "1.15.4",
    "sinon-chai": "2.8.0",
    "with-package": "0.2.0"
  },
  "dependencies": {
    "inquirer": "^1.1.3",
    "lodash.map": "^4.6.0",
    "longest": "^2.0.0",
    "right-pad": "^1.0.1",
    "word-wrap": "^1.2.2"
  },
  "config": {
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert",
        "ci",
        "build"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 100,
      "subjectPattern": ".+",
      "subjectPatternErrorMsg": "subject does not match subject pattern!",
      "helpMessage": ""
    },
    "commitizen": {
      "path": "./index"
    }
  }
}
